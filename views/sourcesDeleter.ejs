<form action="/admin/sources/delete" method="POST">

    <label for="deleteSource"><b>Choose the Sources To Be Deleted</b></label> <br>
    <% for (let i=0; i<jsonData.rssSources.length; i++) { %>
        <input type="checkbox" id="<%=jsonData.rssSources[i].title%>" name="deleteSource" value="<%=jsonData.rssSources[i].title%>">
        <label for="<%=jsonData.rssSources[i].title%>"> <%=jsonData.rssSources[i].title%></label>
        <br>
    <% } %>

    <br>
    <input type="submit" value="Confirm Deletion" />

</form>